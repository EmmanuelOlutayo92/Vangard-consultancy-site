{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/favicon-32x32.png", "/favicon-16x16.png", "/apple-touch-icon.png", "/vangard-logo.svg", "/_nuxt/*", "/robots.txt"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "Referrer-Policy": "strict-origin-when-cross-origin"
  }
}
